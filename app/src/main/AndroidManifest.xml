<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.hazira.chatsiskomdig">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:toolsignore="GoogleAppIndexingWarning">
        <activity
                android:name="com.hazira.chatsiskomdig.MainActivity"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.hazira.chatsiskomdig.M1Activity"
                android:theme="@style/AppTheme.NoActionBar" />
        <activity
                android:name="com.hazira.chatsiskomdig.M2Activity"
                android:theme="@style/AppTheme.NoActionBar" />
        <activity
                android:name="com.hazira.chatsiskomdig.ConsoleActivity"
                android:theme="@style/AppTheme.NoActionBar" />
        <activity
                android:name="com.hazira.chatsiskomdig.CodingActivity"
                android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.hazira.chatsiskomdig.M1Q1Activity" />
        <activity android:name="com.hazira.chatsiskomdig.Quiz1Activity"
                  android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="co.chatsdk.ui.chat.LoginActivity" />


    </application>



</manifest>